<?xml version='1.0' encoding='utf-8'?>
<widget defaultlocale="en" id="org.etive.city4age.app" version="0.1.2" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="BLE">
        <param name="android-package" value="com.evothings.BLE" />
    </feature>
    <feature name="NetworkStatus">
        <param name="android-package" value="org.apache.cordova.networkinformation.NetworkManager" />
    </feature>
    <feature name="Device">
        <param name="android-package" value="org.apache.cordova.device.Device" />
    </feature>
    <feature name="BackgroundMode">
        <param name="android-package" value="de.appplant.cordova.plugin.background.BackgroundMode" />
    </feature>
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <feature name="Notification">
        <param name="android-package" value="org.apache.cordova.dialogs.Notification" />
    </feature>
    <feature name="AutoStart">
        <param name="android-package" value="com.tonikorin.cordova.plugin.autostart.AutoStart" />
    </feature>
    <feature name="BackgroundGeolocation">
        <param name="android-package" value="com.tenforwardconsulting.cordova.BackgroundGeolocationPlugin" />
    </feature>
    <name>City4Age</name>
    <description>
        An app that detects Eddystone beacons and sends proximity info to the City4Age Repository.
    </description>
    <author email="p.crooks@etive.org" href="http://www.etive.org">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <access origin="*" />
    <preference name="loglevel" value="DEBUG" />
    <preference name="AndroidLaunchMode" value="singleInstance" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="HideKeyboardFormAccessoryBar" value="true" />
    <preference name="KeepRunning" value="true" />
    <preference name="android-minSdkVersion" value="18" />
    <preference name="android-targetSdkVersion" value="24" />
</widget>
